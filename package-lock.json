{
  "name": "monorepo",
  "version": "1.0.0",
  "lockfileVersion": 2,
  "requires": true,
  "packages": {
    "": {
      "name": "monorepo",
      "version": "1.0.0",
      "workspaces": [
        "./packages/*"
      ],
      "devDependencies": {},
      "engines": {
        "node": "16.x"
      }
    },
    "node_modules/@monorepo/products": {
      "resolved": "packages/products",
      "link": true
    },
    "node_modules/@monorepo/users": {
      "resolved": "packages/users",
      "link": true
    },
    "packages/products": {
      "name": "@monorepo/products",
      "version": "1.0.0"
    },
    "packages/users": {
      "name": "@monorepo/users",
      "version": "1.0.0"
    }
  },
  "dependencies": {
    "@monorepo/products": {
      "version": "file:packages/products"
    },
    "@monorepo/users": {
      "version": "file:packages/users"
    }
  }
}
